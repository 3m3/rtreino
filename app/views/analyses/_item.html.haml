- content_tag_for(:tr, analysis, :class => cycle(:odd, :even)) do
  %td.body= h analysis.try(:body)
  %td.user_id= h analysis.try(:user_id)
  %td.problem_id= h analysis.try(:problem_id)
  %td.actions
    = link_to 'Show', analysis_path(analysis)
    |
    = link_to 'Edit', edit_analysis_path(analysis)
    |
    = link_to 'Destroy', analysis, :confirm => 'Are you sure?', :method => :delete